<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <typedef class="com.liteoc.domain.enumsupport.CodedEnumType" name="status">
        <param name="enumClassname">com.liteoc.domain.Status</param>
    </typedef>
    <typedef class="com.liteoc.domain.enumsupport.CodedEnumType" name="ruleContext">
        <param name="enumClassname">com.liteoc.domain.rule.expression.Context</param>
    </typedef>
    <typedef class="com.liteoc.domain.enumsupport.CodedEnumType" name="actionType">
        <param name="enumClassname">com.liteoc.domain.rule.action.ActionType</param>
    </typedef>
    <typedef class="com.liteoc.domain.enumsupport.CodedEnumType" name="loginStatus">
        <param name="enumClassname">com.liteoc.domain.technicaladmin.LoginStatus</param>
    </typedef>
</hibernate-mapping>